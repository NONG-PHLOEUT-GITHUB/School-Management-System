{
  "name": "school-management",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint --ext .js,.vue .",
    "preview": "vite preview",
    "clean": "rd /s /q node_modules && del package-lock.json",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,vue}'",
    "format:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,vue}'"
  },
  "dependencies": {
    "@mdi/font": "^7.2.96",
    "axios": "^1.6.0",
    "chart.js": "^4.4.0",
    "js-cookie": "^3.0.5",
    "pinia": "^2.1.7",
    "sass": "^1.69.5",
    "vue": "^3.2.13",
    "vue-chartjs": "^5.2.0",
    "vue-i18n": "^9.6.2",
    "vue-router": "^4.0.3",
    "vuetify": "^3.0.0-beta.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.12.16",
    "@vitejs/plugin-vue": "^1.10.2",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^8.7.1",
    "prettier": "3.1.0",
    "prettier-plugin-vue": "^1.1.6",
    "vite": "^2.9.16",
    "vue-cli-plugin-vuetify": "~2.5.8"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/multi-word-component-names": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
